{
  "name": "@caves/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview --port 5173",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint:fix": "eslint . --fix",
    "check": "npm run lint && npm run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "visual:test": "npm run storybook:build && chromatic --exit-zero-on-changes --storybook-build-dir storybook-static",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@heroicons/react": "^2.1.5",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-table": "^8.20.5",
    "ajv": "^8.17.1",
    "apexcharts": "^5.3.5",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.6",
    "lucide-react": "^0.453.0",
    "postcss": "^8.5.6",
    "quill": "^1.3.7",
    "react": "^18",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18",
    "react-router-dom": "^6.26.2",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.2.0",
    "@storybook/addon-essentials": "^8.2.0",
    "@storybook/addon-interactions": "^8.2.0",
    "@storybook/react": "^8.2.0",
    "@storybook/react-vite": "^8.2.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.3.1",
    "chromatic": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "storybook": "^8.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.44.0",
    "vite": "^5.4.8",
    "vitest": "^1.6.0",
    "cypress": "^13.13.0"
  }
}

